{
  "name": "echarts-with-react",
  "version": "0.1.1",
  "description": "ECharts React wrapper",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linyxria/react-echarts.git"
  },
  "license": "MIT",
  "keywords": [
    "echarts",
    "react"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-echarts.umd.cjs",
  "module": "./dist/react-echarts.js",
  "types": "./dist/react-echarts.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-scale-adjust.js",
      "require": "./dist/react-scale-adjust.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "echarts": "^5.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "echarts": ">=5",
    "react": ">=17",
    "react-dom": ">=17"
  }
}
